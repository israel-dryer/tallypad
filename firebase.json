{
  "hosting": {
    "public": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/catandice",
        "destination": "https://catandice.tallypad.net",
        "type": 301
      },
      {
        "source": "/catandice/**",
        "destination": "https://catandice.tallypad.net/:splat",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
